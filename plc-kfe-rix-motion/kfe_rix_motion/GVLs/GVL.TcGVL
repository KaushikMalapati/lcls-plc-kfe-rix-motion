    // For devices after the stopper
    {attribute 'pytmc' := 'pv: PLC:RIX:MOTION:FFO:02'}
    {attribute 'TcLinkTo' := '.q_xFastFaultOut:=TIIB[PMPS_FFO]^Channel 2^Output'}
