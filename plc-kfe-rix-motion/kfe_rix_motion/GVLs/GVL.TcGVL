    fPhotonEnergy_eV AT %I* : REAL;
	{attribute 'pytmc' := 'pv: PLC:RIX:MOTION:PMPS:ReqTrans'}
	rReqTrans AT %I*		: ARRAY [1..2] OF ST_PMPS_Attenuator_IO;
	{attribute 'pytmc' := 'pv: PLC:RIX:MOTION:PMPS:CurTrans'}
	rCurTrans AT %Q*		: ARRAY [1..2] OF ST_PMPS_Attenuator_IO;
	
END_VAR

VAR_GLOBAL CONSTANT
    iFiltersPerSATTBlade : INT := 8;
END_VAR]]></Declaration>
